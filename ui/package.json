{
  "name": "ui",
  "version": "1.0.0",
  "description": "Snnap react front-end",
  "keywords": [],
  "author": "Max Saperstone",
  "license": "ISC",
  "private": true,
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "lines": 95
      }
    },
    "coverageReporters": [
      "lcov",
      "text",
      "cobertura"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}",
      "!**/index.jsx"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "junit.xml"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint ./src/ -f @microsoft/eslint-formatter-sarif -o ui-results.sarif",
    "owasp": "owasp-dependency-check --scan package-lock.json --project 'snnap-ui' -f ALL --failOnCVSS 8",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:ci": "react-scripts test --silent --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "proxy": "https://localhost:3001",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@geoapify/geocoder-autocomplete": "^1.3.4",
    "@geoapify/react-geocoder-autocomplete": "^1.3.1",
    "bootstrap": "^5.2.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.7.0",
    "react-bootstrap-icons": "^1.10.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.1",
    "react-icons": "^4.7.1",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "eslint": "8.34.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-html-reporter": "^3.7.0",
    "jest-junit": "^15.0.0",
    "jest-websocket-mock": "^2.4.0",
    "owasp-dependency-check": "^0.0.21",
    "prettier": "^2.8.3"
  },
  "overrides": {
    "nth-check": "2.1.1",
    "mismatch": "3.0.5",
    "loader-utils": "2.0.4"
  }
}
