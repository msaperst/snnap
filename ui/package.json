{
  "name": "ui",
  "version": "0.1.0",
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "lines": 95
      }
    },
    "coverageReporters": [
      "lcov",
      "text",
      "cobertura"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}",
      "!**/index.jsx"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "junit.xml"
  },
  "private": true,
  "dependencies": {
    "@geoapify/geocoder-autocomplete": "^1.3.4",
    "@geoapify/react-geocoder-autocomplete": "^1.3.1",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.2.0",
    "owasp-dependency-check": "^0.0.21",
    "react": "^17.0.2",
    "react-bootstrap": "^2.5.0",
    "react-bootstrap-icons": "^1.8.4",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-places-autocomplete": "^7.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.4.0",
    "rxjs": "^7.5.6",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint ./src/",
    "owasp": "owasp-dependency-check --scan package-lock.json --project 'number-muncher' -f HTML JSON",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:ci": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "enzyme": "^3.11.0",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-html-reporter": "^3.6.0",
    "jest-junit": "^14.0.0",
    "prettier": "^2.7.1"
  }
}
