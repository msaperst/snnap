{
  "name": "ui",
  "version": "1.0.0",
  "description": "Snnap react front-end",
  "keywords": [],
  "author": "Max Saperstone",
  "license": "ISC",
  "private": true,
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "lines": 95
      }
    },
    "coverageReporters": [
      "lcov",
      "text",
      "cobertura"
    ],
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}",
      "!**/index.jsx"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "junit.xml"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint ./src/ -f @microsoft/eslint-formatter-sarif -o ui-results.sarif",
    "owasp": "owasp-dependency-check --scan package-lock.json --project 'snnap-ui' -f ALL --failOnCVSS 8",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "test:ci": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "proxy": "https://localhost:3001",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@geoapify/geocoder-autocomplete": "^1.3.4",
    "@geoapify/react-geocoder-autocomplete": "^1.3.1",
    "bootstrap": "^5.2.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.5.0",
    "react-bootstrap-icons": "^1.9.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.6.0",
    "react-places-autocomplete": "^7.3.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "react-select": "^5.5.8",
    "rxjs": "^7.5.7",
    "web-vitals": "^3.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@microsoft/eslint-formatter-sarif": "^3.0.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.5.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "enzyme": "^3.11.0",
    "eslint": "8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-html-reporter": "^3.7.0",
    "jest-junit": "^14.0.1",
    "owasp-dependency-check": "^0.0.21",
    "prettier": "^2.7.1"
  },
  "overrides": {
    "nth-check": "2.1.1",
    "mismatch": "3.0.5",
    "loader-utils": "2.0.3"
  }
}
